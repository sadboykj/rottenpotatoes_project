<!-- reviews-index -->
<h1>
    Reviews
</h1>

<a href="/reviews/new">
    New Review
</a>

{{#each reviews}}

  <h2>
      <a href="/reviews/{{this._id}}">
          {{this.title}}
      </a>
  </h2>

  <h4>
      {{this.movieTitle}}
  </h4>

  <h4>
      Rating: {{this.rating}}
  </h4>

{{/each}}
